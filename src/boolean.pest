WHITESPACE = _{ " " | "\t" | "\n" }

expr       = { or }
or         = { xnor ~ ( "|" ~ xnor )* }
xnor       = { xor ~ ( "!^" ~ xor )*}
xor        = { and ~ ( "^" ~ and )* }
and        = { not ~ (("*" | "&") ~ not)* }
not        = { ("'" | "!")* ~ primary }
primary    = { ident | "(" ~ expr ~ ")" }
ident      = @{ ASCII_ALPHANUMERIC+ }
